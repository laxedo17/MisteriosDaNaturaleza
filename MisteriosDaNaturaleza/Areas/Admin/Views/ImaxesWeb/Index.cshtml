
@{

    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<div class="border backgroundWhite">
    <div class="row">
        <div class="col-6">
            <h2 class="text-primary">Lista de Imaxes</h2>
        </div>
        <div class="col-6 text-right">
            <a asp-action="ActualInsertar" class="btn btn-primary"><i class="fas fa-plus"></i> &nbsp; Subir nova imaxen</a>
        </div>
    </div>

    <table id="tblData" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>Nome</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{
    <script src="~/js/imaxesweb.js"></script>
}

